{
  "name": "games",
  "version": "1.0.0",
  "main": "server/index.ts",
  "license": "MIT",
  "private": true,
  "scripts": {
    "server": "nodemon ./server/index.ts",
    "start": "DATABASE_URL=postgres://postgres:postgres@localhost:5432/crypto_games npm run server",
    "startLocal": "DATABASE_URL=postgres://wacker:@localhost:5432/crypto_games npm run server",
    "build": "webpack --config webpack.config.js --color --verbose --progress --watch",
    "typeorm:cli": "ts-node ./node_modules/typeorm/cli -f ./ormconfig.ts",
    "typeorm": "ts-node -r ./ormconfig.ts ./node_modules/.bin/typeorm",
    "migration:generate": "npm run typeorm:cli -- migration:generate --connection  --name ",
    "migration:run": "npm run typeorm:cli -- migration:run",
    "migration:revert": "npm run typeorm:cli -- migration:revert",
    "test": "mocha --timeout 10000"
  },
